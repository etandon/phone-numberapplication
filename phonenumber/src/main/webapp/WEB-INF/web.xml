<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Etandon Application</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>This is the Main Servelet of the Application</description>
    <display-name>AppPage</display-name>
    <servlet-name>AppPage</servlet-name>
    <servlet-class>com.etandon.phonenumber.servelets.AppPages</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AppPage</servlet-name>
    <url-pattern>/app</url-pattern>
  </servlet-mapping>
</web-app>